                "alias": "ns",
                "name": "#__notification_subscribers",
                "on": "ngs.subscriber_id = ns.id"
            },
            {
                "table": "Notification\\Groups\\Code\\Tables\\Groups",
                "alias": "ng",
                "name": "#__notification_groups",
                "on": "ngs.group_id = ng.id"
