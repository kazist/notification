    "query": {
        "from": {
            "table": "Notification\\Newsletters\\Automated\\Code\\Tables\\Automated",
            "alias": "nna",
            "name": "#__notification_newsletters_automated"
        },
        "select": [
            "nna.id",
            "nna.subset_id",
            "ss.title AS subset_id_title",
            "nna.frequency_id",
            "nnf.title AS frequency_id_title",
            "nna.unique_name",
