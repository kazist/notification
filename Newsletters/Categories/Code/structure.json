    "query": {
        "from": {
            "table": "Notification\\Newsletters\\Categories\\Code\\Tables\\Categories",
            "alias": "nnc",
            "name": "#__notification_newsletters_categories"
        },
        "select": [
            "nnc.id",
            "nnc.parent_id",
            "nnc_4218.title AS parent_id_title",
            "nnc.frequency_id",
            "nnf.title AS frequency_id_title",
            "nnc.title",
            "nnc.description",
            "nnc.image",
            "mm.title AS image_title",
            "mm.file AS image_file",
            "nnc.created_by",
            "nnc.date_created",
            "nnc.modified_by",
            "nnc.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Notification\\Newsletters\\Categories\\Code\\Tables\\Categories",
                "alias": "nnc_4218",
                "name": "#__notification_newsletters_categories",
                "on": "nnc.parent_id = nnc_4218.id"
            },
            {
                "table": "Notification\\Newsletters\\Frequencies\\Code\\Tables\\Frequencies",
                "alias": "nnf",
                "name": "#__notification_newsletters_frequencies",
                "on": "nnc.frequency_id = nnf.id"
            },
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "nnc.image = mm.id"
            }
        ]
    }
