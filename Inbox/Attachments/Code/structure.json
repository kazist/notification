    "query": {
        "from": {
            "table": "Notification\\Inbox\\Attachments\\Code\\Tables\\Attachments",
            "alias": "nia",
            "name": "#__notification_inbox_attachments"
        },
        "select": [
            "nia.id",
            "nia.inbox_id",
            "ni.subject AS inbox_id_subject",
            "nia.file",
            "mm.title AS file_title",
            "mm.file AS file_file",
            "nia.date_created",
            "nia.date_modified",
            "nia.modified_by",
            "uu.name AS modified_by_name",
            "nia.created_by",
            "uu_5209.name AS created_by_name"
        ],
        "leftjoin": [
            {
                "table": "Notification\\Inbox\\Code\\Tables\\Inbox",
                "alias": "ni",
                "name": "#__notification_inbox",
                "on": "nia.inbox_id = ni.id"
            },
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "nia.file = mm.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "nia.modified_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_5209",
                "name": "#__users_users",
                "on": "nia.created_by = uu_5209.id"
            }
        ]
    }
