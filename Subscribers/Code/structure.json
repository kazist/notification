    "query": {
        "from": {
            "table": "Notification\\Subscribers\\Code\\Tables\\Subscribers",
            "alias": "ns",
            "name": "#__notification_subscribers"
        },
        "select": [
            "ns.id",
            "ns.name",
            "ns.email",
            "ns.user_id",
            "uu.name AS user_id_name",
            "ns.ordering",
            "ns.published",
            "ns.created_by",
            "uu_1242.name AS created_by_name",
            "ns.date_created",
            "ns.modified_by",
            "uu_6266.name AS modified_by_name",
            "ns.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "ns.user_id = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_1242",
                "name": "#__users_users",
                "on": "ns.created_by = uu_1242.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_6266",
                "name": "#__users_users",
                "on": "ns.modified_by = uu_6266.id"
            }
        ]
    }
