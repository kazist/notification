    "query": {
        "from": {
            "table": "Notification\\Subscribers\\Groups\\Code\\Tables\\Groups",
            "alias": "nsg",
            "name": "#__notification_subscribers_groups"
        },
        "select": [
            "nsg.id",
            "nsg.subset_id",
            "ss.title AS subset_id_title",
            "nsg.name",
            "nsg.description",
            "nsg.ordering",
            "nsg.published",
            "nsg.created_by",
            "uu.name AS created_by_name",
            "nsg.date_created",
            "nsg.modified_by",
            "uu_8758.name AS modified_by_name",
            "nsg.date_modified"
        ],
        "leftjoin": [
            {
                "table": "System\\Subsets\\Code\\Tables\\Subsets",
                "alias": "ss",
                "name": "#__system_subsets",
                "on": "nsg.subset_id = ss.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "nsg.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_8758",
                "name": "#__users_users",
                "on": "nsg.modified_by = uu_8758.id"
            }
        ]
    }
