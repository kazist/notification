    "query": {
        "from": {
            "table": "Notification\\Subscribers\\Groups\\Subscribers\\Code\\Tables\\Subscribers",
            "alias": "nsgs",
            "name": "#__notification_subscribers_groups_subscribers"
        },
        "select": [
            "nsgs.id",
            "nsgs.subscriber_id",
            "ns.title AS subscriber_id_title",
            "nsgs.group_id",
            "nsg.title AS group_id_title",
            "nsgs.created_by",
            "uu.name AS created_by_name",
            "nsgs.date_created",
            "nsgs.modified_by",
            "uu_6465.name AS modified_by_name",
            "nsgs.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Notification\\Subscribers\\Code\\Tables\\Subscribers",
                "alias": "ns",
                "name": "#__notification_subscribers",
                "on": "nsgs.subscriber_id = ns.id"
            },
            {
                "table": "Notification\\Subscribers\\Groups\\Code\\Tables\\Groups",
                "alias": "nsg",
                "name": "#__notification_subscribers_groups",
                "on": "nsgs.group_id = nsg.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "nsgs.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_6465",
                "name": "#__users_users",
                "on": "nsgs.modified_by = uu_6465.id"
            }
        ]
    }
