    "query": {
        "from": {
            "table": "Notification\\Subscribers\\Harvesters\\Code\\Tables\\Harvesters",
            "alias": "nsh",
            "name": "#__notification_subscribers_harvesters"
        },
        "select": [
            "nsh.id",
            "nsh.subset_id",
            "ss.title AS subset_id_title",
            "nsh.title",
            "nsh.email_field",
            "nsh.user_field",
            "nsh.extension_path",
            "nsh.created_by",
            "uu.name AS created_by_name",
            "nsh.date_created",
            "nsh.modified_by",
            "uu_5721.name AS modified_by_name",
            "nsh.date_modified"
        ],
        "leftjoin": [
            {
                "table": "System\\Subsets\\Code\\Tables\\Subsets",
                "alias": "ss",
                "name": "#__system_subsets",
                "on": "nsh.subset_id = ss.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "nsh.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_5721",
                "name": "#__users_users",
                "on": "nsh.modified_by = uu_5721.id"
            }
        ]
    }
