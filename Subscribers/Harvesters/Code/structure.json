    "query": {
        "from": {
            "table": "Notification\\Subscribers\\Harvesters\\Code\\Tables\\Harvesters",
            "alias": "nsh",
            "name": "#__notification_subscribers_harvesters"
        },
        "select": [
            "nsh.id",
            "nsh.title",
            "nsh.group_id",
            "ng.name AS group_id_name",
            "nsh.email_field",
            "nsh.user_field",
            "nsh.extension_path",
            "nsh.published",
            "nsh.created_by",
            "uu.name AS created_by_name",
            "nsh.date_created",
            "nsh.modified_by",
            "uu_4234.name AS modified_by_name",
            "nsh.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Notification\\Groups\\Code\\Tables\\Groups",
                "alias": "ng",
                "name": "#__notification_groups",
                "on": "nsh.group_id = ng.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu",
                "name": "#__users_users",
                "on": "nsh.created_by = uu.id"
            },
            {
                "table": "Users\\Users\\Code\\Tables\\Users",
                "alias": "uu_4234",
                "name": "#__users_users",
                "on": "nsh.modified_by = uu_4234.id"
            }
        ]
    }
