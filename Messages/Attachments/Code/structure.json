    "query": {
        "from": {
            "table": "Notification\\Messages\\Attachments\\Code\\Tables\\Attachments",
            "alias": "nma",
            "name": "#__notification_messages_attachments"
        },
        "select": [
            "nma.id",
            "nma.media_id",
            "mm.title AS media_id_title",
            "mm.file AS media_id_file",
            "nma.message_id",
            "nm.message AS message_id_message",
            "nma.type",
            "nma.created_by",
            "nma.date_created",
            "nma.modified_by",
            "nma.date_modified"
        ],
        "leftjoin": [
            {
                "table": "Media\\Media\\Code\\Tables\\Media",
                "alias": "mm",
                "name": "#__media_media",
                "on": "nma.media_id = mm.id"
            },
            {
                "table": "Notification\\Messages\\Code\\Tables\\Messages",
                "alias": "nm",
                "name": "#__notification_messages",
                "on": "nma.message_id = nm.id"
            }
        ]
    }
